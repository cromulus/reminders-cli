{
  "name": "n8n-nodes-reminders-api",
  "version": "1.0.0",
  "description": "n8n nodes for macOS Reminders API - Create, manage, and search reminders with advanced features",
  "keywords": [
    "n8n-community-node-package",
    "reminders",
    "macos",
    "productivity",
    "task-management"
  ],
  "license": "MIT",
  "homepage": "https://github.com/your-username/n8n-nodes-reminders-api",
  "author": {
    "name": "Your Name",
    "email": "your-email@example.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/n8n-nodes-reminders-api.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/RemindersApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Reminders/Reminders.node.js",
      "dist/nodes/SearchReminders/SearchReminders.node.js",
      "dist/nodes/WebhookManager/WebhookManager.node.js",
      "dist/nodes/RemindersTrigger/RemindersTrigger.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^18.16.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.42.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
